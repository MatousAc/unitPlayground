{
  "name": "unitplayground",
  "license": "MIT",
  "version": "0.2.1",
  "homepage": "unitplayground.com",
  "repository": {
    "type": "git",
    "url": "github.com/MatousAc/unitPlayground"
  },
  "keywords": [
    "dimensional analysis",
    "generative dimensional analysis",
    "interactive unit math",
    "unit conversion"
  ],
  "scripts": {
    "dev": "vite dev",
    "preview": "vite preview",
    "build": "vite build && npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "publint": "^0.1.9",
    "rollup": "^3.20.2",
    "rollup-plugin-svelte": "^7.1.4",
    "svelte": "^3.54.0",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@cortex-js/compute-engine": "^0.12.2",
    "@neodrag/svelte": "^2.0.3",
    "@supabase/auth-helpers-sveltekit": "^0.9.0",
    "@supabase/supabase-js": "^2.13.1",
    "unitmath": "^0.8.8"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
