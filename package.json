{
  "name": "unitplayground",
  "license": "MIT",
  "version": "0.2.4",
  "homepage": "unitplayground.com",
  "repository": {
    "type": "git",
    "url": "github.com/MatousAc/unitPlayground"
  },
  "keywords": [
    "dimensional analysis",
    "generative dimensional analysis",
    "interactive unit math",
    "unit conversion"
  ],
  "scripts": {
    "dev": "vite dev",
    "preview": "vite preview",
    "build": "vite build && npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^4.2.7"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.30.4",
    "@sveltejs/package": "^2.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-highlight": "^7.2.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "universal-cookie": "^4.0.4",
    "vite": "^4.5.1"
  },
  "dependencies": {
    "@cortex-js/compute-engine": "^0.12.2",
    "@neodrag/svelte": "^2.0.3",
    "@supabase/supabase-js": "^2.13.1",
    "unitmath": "^0.8.8"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
